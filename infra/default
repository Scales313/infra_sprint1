 server {
    server_name 158.160.21.114 justsite.ddnsking.com;

    location /api/ {
        proxy_pass http://127.0.0.1:8000;
    }
    
    location /admin/ {
        proxy_pass http://127.0.0.1:8000;
    }

    location / {
        root   /var/www/taski;
        index  index.html index.htm;
        try_files $uri /index.html;
    }


    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/justsite.ddnsking.com/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/justsite.ddnsking.com/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

} 

server {
    server_name 158.160.21.114 ktgmmm.ddns.net;

    location /admin/ {
        proxy_pass http://127.0.0.1:8080;
    }

    location /api/ {
        proxy_pass http://127.0.0.1:8080;
    }

    location / {
        root /var/www/kittygram;
        index index.html index.htm;
        try_files $uri /index.html;
    }

    location /media/ {
        alias /var/www/kittygram/media/;
    }

    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/ktgmmm.ddns.net/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/ktgmmm.ddns.net/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}
 

server {
    if ($host = ktgmmm.ddns.net) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    listen 80;
    server_name 158.160.21.114 ktgmmm.ddns.net;
    return 404; # managed by Certbot


} server {
    if ($host = justsite.ddnsking.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot



    listen 80;
    server_name 158.160.21.114 justsite.ddnsking.com;
    return 404; # managed by Certbot


}